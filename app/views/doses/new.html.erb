<h1 id="show-heading">Add a dose and ingredient</h1>

<%= simple_form_for [@cocktail, @dose] do |f| %>
  <%= f.input :description %>
   <%= f.association :ingredient %>
  <%= f.submit "Add description and Ingredient", class: "btn btn-primary" %>
<% end %>

<%= link_to "Back to list", cocktails_path, class: "btn btn-primary"  %>
